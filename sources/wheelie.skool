c$ED39 CALL $7552    ; Call #R$7552.
; Create an offset using #REGbc.
 $ED3C LD A,($7820)  ; {#REGc=*#R$7820*#N$05.
 $ED3F LD B,A        ;
 $ED40 ADD A,A       ;
 $ED41 ADD A,A       ;
 $ED42 ADD A,B       ;
